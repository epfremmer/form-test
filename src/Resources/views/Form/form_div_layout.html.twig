{%- extends 'form_div_layout.html.twig' -%}

{%- block form_widget -%}
    {%- set data_prototypes = [] -%}

    {%- for key, prototype in prototypes -%}
        {%- set data_prototypes = data_prototypes|merge({(key): form_row(prototype)}) -%}
    {%- endfor -%}

    {%- if data_prototypes is not empty -%}
        {%- set attr = attr|merge({'data-prototypes' : data_prototypes|json_encode }) -%}
    {%- endif -%}

    {{ parent() }}
{%- endblock form_widget -%}
